SELECT
  PARSE_TIMESTAMP("%y%m%d%k%M", CONCAT(DA,TI)) timestamp,
  RC AS callsign,
  NA AS shipname,
  RN AS registry_number,
  IR AS internal_id,
  XR AS external_id,
  CAST(LT AS FLOAT64) lat,
  CAST(LG AS FLOAT64) lon,
  CAST(SP AS FLOAT64) speed,
  CAST(CO AS FLOAT64) course,
  FS AS flag,
  IM as imo,
  ID as transmitter_id,
  SQ as sequence_number,
FROM
  `{{ source }}`
